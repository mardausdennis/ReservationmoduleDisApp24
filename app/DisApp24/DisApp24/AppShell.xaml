<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DisApp24.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DisApp24"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="RSS" Icon="AppIcon/rss_icon.png">
            <ShellContent ContentTemplate="{DataTemplate local:RssPage}" />
        </Tab>
        <Tab Title="Reservation" Icon="AppIcon/calendar_icon.png">
            <ShellContent  Route="reservationpage"  ContentTemplate="{DataTemplate local:ReservationPage}" />
        </Tab>
    </TabBar>
</Shell>
